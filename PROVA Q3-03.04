<script>
var salario = prompt("Digite seu salário")
var vendas = prompt("Digite suas vendas")

if(Number(salario) + Number(vendas) > 5000){
var salarioT = (Number(salario) + Number(vendas)) * 0.03
alert("Seu salário total é de: ") + salarioT)
} else {
var salarioT1 = (Number(salario) + Number(vendas)) * 0.03
var salarioT2 = ((Number(salario) + Number(vendas)) - 5000) * 0.05
var salarioT = Number(salarioT1) + Number(salarioT2)
alert("Seu salário total é de: " + salarioT)
</script>

<!DOCTYPE html>
<html>
<body>

<script>

var velLimite = prompt("Digite a velocidade limite da estrada:")
var velCondutor = prompt("Digite a velocidade do condutor na estrada:")

var diferenca = Math.abs(velCondutor - velLimite) / velLimite * 100;

if(velCondutor<=velLimite){
	alert("Sem Multa")
} else if(diferenca<=20){
	alert("Multa leve")
} else {
	alert("Multa Grave")
}
</script>



</body>
</html>

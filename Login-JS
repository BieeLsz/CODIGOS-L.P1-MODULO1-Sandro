<!DOCTYPE html>
<html>
<body>
<script>
const senhaCorreta = "110264";
var i = 0;

var nome = prompt("Digite o seu nome:");

while (i < 3) {
  var senha = prompt("Digite a senha:");

  if (senha === senhaCorreta) {
    alert("Senha correta! Acesso concedido "+ nome +".");
    break;
  } else {
    i++;
    alert("Senha incorreta. Tentativa "+i+".");
  }
}
if(i === 3){
alert("Você excedeu o número máximo de tentativas. Acesso negado.");
}
</script>
</body>
</html>
